define host{
        use             	windows-server  ; Inherit default values from a template
        host_name       	<%= hostname%>    ; The name we're giving to this host
        alias           	<%= @fqdn%>       ; A longer name associated with the host
        address         	<%= @getip%>     ; IP address of the host
        hostgroups      	<%= machine_app%>
        contacts        	<%= scope.lookupvar('nagios_devops::setup::techsqlpd_template') %>, <%= scope.lookupvar('nagios_devops::setup::contactgroup') %> 
        }

define service{
        use                     generic-service
        host_name               <%= @hostname%>
        service_description     NSC DevOps Client
        servicegroups           Nagios
        contacts                DevOps
	check_command           check_nt!SERVICESTATE!-d SHOWALL -l nsc-devops
        }

define service{
        use                     generic-service
        host_name               <%= @hostname%>
        service_description     CPU Load
        servicegroups           CPU
        contacts        	<%= scope.lookupvar('nagios_devops::setup::contactgroup') %>
        check_command           check_nt!CPULOAD!-l 60,90,100
        }

define service{
        use                     generic-service
        host_name               <%= @hostname%>
        service_description     Puppet Agent Enabled
        servicegroups           Puppet
        notification_interval   86400
        contacts                DevOps
        check_command           check_puppet_nagios!
        }

define service{
        use                     generic-service
        host_name               <%= @hostname%>
        service_description     SQL Analysis Service OLP
        servicegroups           SQL
        check_command           check_nt!SERVICESTATE!-d SHOWALL -l MSSQLServerOLAPService
        }
		
